	  <script language="javascript">
	  
	  var approvednoedit = false;
	  var approvednoprint = false;
	  
	  function checkifpoapproved(nomsg)
	  {
		  if (self.approvednoedit && self.approvednoedit == true)
		  {
				if (nomsg == null || nomsg == false)
				{
					var amessage = 'This PO has been approved. You may not edit this field or access this feature once the PO has been approved.';
					top.mcalert('warning','No Access:',amessage,'bg_ok',600,240,'sounds/error.wav');		
				}
				self.focus();
				return false;								 
		  }
		  else
		  {
				return true;
		  }
	  }
	  function checkifpocanprint(nomsg)
	  {
	      return true;
	      
		  if (self.approvednoprint && self.approvednoprint == true)
		  {
				if (nomsg == null || nomsg == false)
				{
					var amessage = 'This PO has not been approved. You may not perform this function until the PO has been approved.';
					top.mcalert('warning','No Access:',amessage,'bg_ok',600,240,'sounds/error.wav');		
				}
				self.focus();
				return false;								 
		  }
		  else
		  {
				return true;
		  }
	  }	  
	  function checkifpocanrecv(nomsg)
	  {
		  if (self.approvednoprint && self.approvednoprint == true)
		  {
				if (nomsg == null || nomsg == false)
				{
					var amessage = 'This PO has not been approved. You may not perform this function until the PO has been approved.';
					top.mcalert('warning','No Access:',amessage,'bg_ok',600,240,'sounds/error.wav');		
				}
				self.focus();
				return false;								 
		  }
		  else
		  {
		      if (self.notissuedyet && self.notissuedyet == true)
		      {
				    if (nomsg == null || nomsg == false)
				    {
					    var amessage = 'This PO has not been Issued. You may not perform this function until the PO has been Issued.';
					    top.mcalert('warning','No Access:',amessage,'bg_ok',600,240,'sounds/error.wav');		
				    }
				    self.focus();
				    return false;								 
		      }
		      else
		      {		  
				return true;
			  }
		  }
	  }	  	  
	  function checkifallrecv(nomsg)
	  {
		  if (self.OrderUnitQty_TOTAL != self.OrderUnitQtyReceived_TOTAL)
		  {
		        var qo = parseFloat(self.OrderUnitQty_TOTAL);
		        var qr = parseFloat(self.OrderUnitQtyReceived_TOTAL);
		        if (qr < qo)
		        {
		            qtyleft = qo - qr;
		            if (qtyleft == 1)
		            {		        
    					var amessage = 'Not all Items have been received. (There is ' + qtyleft + ' Item that has not been received.)';
    				}
    				else
    				{
    					var amessage = 'Not all Items have been received. (There are ' + qtyleft + ' Items that have not been received.)';    				
    				}
		        }
		        else
		        {
		            qtyleft = qr - qo;
		            if (qtyleft == 1)
		            {		        
					    var amessage = 'Too many Items have been received. (There is ' + qtyleft + ' Item that was received above the ordered quantity.)';
					}
					else
					{
					    var amessage = 'Too many Items have been received. (There are ' + qtyleft + ' Items that were received above the ordered quantity.)';					
					}   
		        }
				if (nomsg == null || nomsg == false)
				{
					top.mcalert('warning','No Access:',amessage,'bg_ok',600,240,'sounds/error.wav');		
				}
				self.focus();
				return false;								 
		  }
		  else
		  {
				return true;
		  }	  
	  }
	  </script>
	  



