    <script type="text/javascript">
        var oldValue = '';

        function ChangeApprovals(inRPK, inPOPK) {

            mcRemotePageFetch('checkAccess.asp', '&rpk=' + inRPK + '&popk=' + inPOPK, function (data) {
                if ($.trim(data) != '') {
                    try {
                        $('#pendingApprovals').show();
                        $('#pendingApprovalData').empty();
                        $('#pendingApprovalData').html(data);
                        //$('.approvalHide').hide();
                    }
                    catch (e) { }
                }
                else {
                    try {
                        $('#pendingApprovals').hide();
                    }
                    catch (e) { }
                }
            }, null);
            //window.open('checkAccess.asp?rpk=' + inRPK + '&popk=' + inPOPK);
        }

        function ValueChanged() {}

        function ForceOverride(popk) {
            try {
                var iRPK = '';
                try {
                    iRPK = myform.txtRepairCenterPK.value;
                }
                catch (e) { }
                ChangeApprovals(iRPK, popk);
            }
            catch (e) { }
        }
		function savePipe()
		{
			d = new Date()
			df = ""+d.getMonth()+d.getDate()+d.getYear()+(d.getHours()+1)+d.getMinutes();
			zStr= "PurchaseOrderDetail.PartID|PurchaseOrderDetail.PartName|PurchaseOrderDetail.OrderUnitQty|PurchaseOrderDetail.OrderUnits|PurchaseOrder.VendorName|PurchaseOrderDetail.OrderUnitPrice|Part.CategoryID|Part.ManufacturerID|PurchaseOrderDetail.AccountID\n"+myform.POExport.value;
			zStr = zStr.replace(/\n/g, "\r\n");
			filename= "PO"+ myform.POName.value +"_" + df + ".txt";
			var element = document.createElement('a');
			element.setAttribute('href', 'data:text/plain;charset=utf-8,' + encodeURIComponent(zStr));
			element.setAttribute('download', filename);
			element.style.display = 'none';
			document.body.appendChild(element);
			element.click();
			document.body.removeChild(element);
}
    </script>



